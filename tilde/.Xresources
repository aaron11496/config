urxvt*termName:rxvt-unicode
urxvt.cursorBlink: true
urxvt*scrollTtyOutput: false
urxvt*scrollTtyKeypress: true
urxvt*saveLines: 10000
urxvt*foreground: white
urxvt*background: black
! urxvt*font: xft:SourceCodePro:pixelsize=13
! urxvt*boldFont: xft:SourceCodePro:bold:pixelsize=13
! urxvt*font: xft:AnonymousPro:pixelsize=14
! urxvt*boldFont: xft:AnonymousPro:bold:pixelsize=14
urxvt*font: xft:Inconsolata:pixelsize=16
urxvt*boldFont: xft:Inconsolata:bold:pixelsize=16

urxvt*scrollBar: false
urxvt*scrollBar_right: true
urxvt*scrollstyle: rxvt
urxvt*scrollBar_floating: true

urxvt.perl-ext-common:  default,matcher
urxvt.urlLauncher:      /usr/bin/google-chrome
urxvt.matcher.button:   2

! Tango Color Scheme
urxvt*color0: #2e3436
urxvt*color1: #cc0000
urxvt*color2: #4e9a06
urxvt*color3: #c4a000
urxvt*color4: #3465a4
urxvt*color5: #75507b
urxvt*color6: #0b939b
urxvt*color7: #d3d7cf
urxvt*color8: #555753
urxvt*color9: #ef2929
urxvt*color10: #8ae234
urxvt*color11: #fce94f
urxvt*color12: #729fcf
urxvt*color13: #ad7fa8
urxvt*color14: #00f5e9
urxvt*color15: #eeeeec
