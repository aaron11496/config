Config { font         = "-xos4-*-*-r-*-*-13-*-*-*-*-*-*-*"
       , bgColor      = "#000000"
       , fgColor      = "#B0B0B0"
       , position     =  Top
       , lowerOnStart = False
       , commands     =
           [
            Run Network "eth0" [ "-L","0","-H","32","-l", "#408040"
                               , "--normal","orange","--high","red" ] 10
           , Run Cpu [ "<cpu0>/<cpu1>","-L","3","-H","50","-l","#408040"
                     , "--normal","orange","--high","red" ]   10
           , Run Cpu [ "<cpu0>","-L","3","-H","50","-l","#408040"
                     , "--normal","orange","--high","red" ]  10
           , Run Memory [ "-t","Mem: <usedratio>%", "-l", "#408040"
                        , "--normal","orange","--high","red" ] 10
           , Run Date "%a %b %_d %H:%M" "date" 60
           , Run StdinReader
           ]
       , sepChar      = "%"
       , alignSep     = "}{"
       , template     = " %StdinReader%  %cpu%  %memory%  %eth0%  %date% "
       }

--, Run CpuFreq ["-t","freq:<cpu0>|<cpu1>GHz","-L","0","-H","2","-l","#408040","-n","orange","-h","red"] 10
