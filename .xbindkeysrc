#keystate_numlock = enable
#keystate_scrolllock = enable
#keystate_capslock = enable



"xbindkeys_show"
   control+shift + q

# volume control buttons rely on volnoti
#Volume Up
"amixer set Master playback '5%+' && volnoti-show `amixer get Master|tail -n1|sed -E 's/.*\[([0-9]+)\%\].*/\1/'`"
   XF86AudioRaiseVolume

#Volume Down
"amixer set Master playback '5%-' && volnoti-show `amixer get Master|tail -n1|sed -E 's/.*\[([0-9]+)\%\].*/\1/'`"
   XF86AudioLowerVolume

#Toggle Mute
"amixer set Master toggle && amixer get Master | grep '\[on\]' && volnoti-show `amixer get Master|tail -n1|sed -E 's/.*\[([0-9]+)\%\].*/\1/'` || volnoti-show -m"
   XF86AudioMute

"dbus-send --print-reply --dest=org.mpris.MediaPlayer2.spotify /org/mpris/MediaPlayer2 org.mpris.MediaPlayer2.Player.PlayPause"
XF86AudioPlay
"dbus-send --print-reply --dest=org.mpris.MediaPlayer2.spotify /org/mpris/MediaPlayer2 org.mpris.MediaPlayer2.Player.Stop"
XF86AudioStop
"dbus-send --print-reply --dest=org.mpris.MediaPlayer2.spotify /org/mpris/MediaPlayer2 org.mpris.MediaPlayer2.Player.Next"
XF86AudioNext
"dbus-send --print-reply --dest=org.mpris.MediaPlayer2.spotify /org/mpris/MediaPlayer2 org.mpris.MediaPlayer2.Player.Previous"
XF86AudioPrev

"spotify"
XF86Tools
"google-chrome"
XF86HomePage


